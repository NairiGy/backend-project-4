{
    "plugins": ["eslint-plugin-jest"],
    "env": {
        "browser": true,
        "es2021": true,
        "jest/globals": true
    },
  "extends": ["airbnb-base", "plugin:jest/recommended"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "import/extensions": [
          "error",
          "ignorePackages",
          {
            "js": "always"
          }
        ]
    }

}
